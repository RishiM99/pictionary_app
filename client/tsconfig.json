{
    "compilerOptions": {
        "esModuleInterop": true,
        "lib": [
            "es2020"
        ],
        "module": "es2022",
        "preserveConstEnums": true,
        "declarationMap": true,
        "declaration": true,
        "moduleResolution": "node",
        "strict": true,
        "sourceMap": true,
        "target": "es2022",
        "types": [
            "node"
        ],
        "outDir": "compiled_javascript",
        "jsx": "react-jsx",
        "noImplicitAny": false
    },
    "include": [
        "**/*.tsx",
    ],
    "exclude": [
        "node_modules",
    ]
}